{
  "compilerOptions": {
                                   
     "lib": ["dom", "dom.iterable", "esnext"],                                  
    "jsx": "react-jsx",                                
    "moduleResolution": "node",   
    "resolveJsonModule": true,      
    "isolatedModules": true,              
    "allowJs": true,        
    "noEmit": true,                                   
    "allowSyntheticDefaultImports": true,             
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,           
    "strict": true,                                      
    "skipLibCheck": true                                 
  },
  "include": ["src"]
}
